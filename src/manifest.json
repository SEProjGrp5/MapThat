{
    "manifest_version": 2,
    "name": "Chrome Extention to add travel time to google calendar",
    "version": "1.0",
    "description": "A chrome extension",  
   "browser_action":{
       "default_popup": "popup.html"
   },
   "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
]
}
